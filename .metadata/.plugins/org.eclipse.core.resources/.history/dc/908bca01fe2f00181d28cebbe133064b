
public class HW02_23 {

	public static void main(String[] args) {
		int first = 5;
		int second = 1;
		int i = 1;
		int k = 1;
		
		while(k <= 3)
		{
			while(first >= 1) {
				System.out.print("*");
				i = i - 1;
			}
				System.out.print(" ");
				i = 1;
			while(second >= 1) {
				System.out.print("*");
				i = i - 1;
			}
			System.out.println(" ");
			
			k++;
		}
		
	}

}
